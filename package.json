{
  "name": "alis-editor",
  "version": "0.0.0",
  "description": "My hunky-dory Vue component.",
  "repository": {
    "url": "potato4d/alis-editor",
    "type": "git"
  },
  "main": "dist/alis-editor.cjs.js",
  "files": ["dist"],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "lint": "eslint . --ext .js --ext .vue",
    "test": "npm run lint && tyu",
    "build": "bili",
    "example": "poi",
    "build:example": "poi build"
  },
  "author": {
    "name": "HANATANI Takuma",
    "email": "mail@potato4d.me"
  },
  "license": "MIT",
  "poi": {
    "entry": "example/index.js",
    "dist": "example/dist",
    "homepage": "./"
  },
  "bili": {
    "format": ["cjs", "umd"],
    "name": "alis-editor",
    "plugins": ["vue"]
  },
  "devDependencies": {
    "poi": "^9.3.10",
    "bili": "^1.3.3",
    "tyu": "^1.0.0",
    "vue-test-utils": "^1.0.0-beta.9",
    "rollup-plugin-vue": "^2.5.2"
  }
}
